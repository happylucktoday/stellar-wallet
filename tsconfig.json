{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "noUnusedLocals": true,
    "skipLibCheck": true
  },
  "rules": {
    "no-submodule-imports": false
  }
}
